<Router AppAssembly="@GetType().Assembly"
AdditionalAssemblies="new[] { typeof(WABCL.Shared.MainLayout).Assembly}">
	<Found Context="routeData">
		<RouteView RouteData="@routeData" DefaultLayout="@typeof(WABCL.Shared.MainLayout)" />
	</Found>
	<NotFound>
		<LayoutView Layout="@typeof(WABCL.Shared.MainLayout)">
			<p>Sorry, there's nothing at this address.</p>
		</LayoutView>
	</NotFound>
</Router>
